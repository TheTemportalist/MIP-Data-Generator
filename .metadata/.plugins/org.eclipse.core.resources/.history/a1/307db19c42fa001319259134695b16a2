package Main.MIP;

public class Item {
	
	public String name;
	public Types type;
	
	public enum Types {
		MATERIAL, FOOD, TOOL, WEAPON, ARMOR, TRANSPORTATION, POTION, MUSICDISC, EGG, OTHER
	}
	
	public Item() {
		this.name = "item";
		this.type = "Material";
		
	}
	
	@Override
	public String toString() {
		return this.name;
	}
	
}
